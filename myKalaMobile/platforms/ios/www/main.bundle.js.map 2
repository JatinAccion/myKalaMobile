Wrapper\\\">\\n                  <img [src]=\\\"mockProfileImage\\\" />\\n                </div>\\n              </div>\\n              <!--Content-->\\n\\n              <!--Footer-->\\n              <div class=\\\"layoutFooter float-left w-100\\\">\\n                <button class=\\\"txtasbutton btn btn-outer btn_red_right btnDone float-left\\\" style=\\\"position: inherit\\\"\\n                  (click)=\\\"uploadImage()\\\">\\n                  upload photo\\n                </button>\\n                <button class=\\\"btn-outer btn_redouter_right btn-outer-margin float-right\\\" style=\\\"position: inherit\\\"\\n                  (click)=\\\"disableEdit('profileImage')\\\">cancel</button>\\n                <button class=\\\"txtasbutton btn btn-outer btn_red_right btnDone float-right mr-2\\\" style=\\\"position: inherit\\\"\\n                  (click)=\\\"saveChange('profileImage')\\\">done</button>\\n              </div>\\n              <!--Footer-->\\n            </div>\\n            <!--Change Layout-->\\n          </div>\\n          <!--Profile Image : Ends-->\\n\\n\\n\\n          <!--Name-->\\n          <div class=\\\"row\\\">\\n            <div class=\\\"col-4 colorlightGray p-2\\\">name</div>\\n            <div class=\\\"col-6 text-right pl-2 pt-2 pb-2 pr-0\\\">{{myAccountModel.profileInfo.firstName}}\\n              {{myAccountModel.profileInfo.lastName}}</div>\\n            <div class=\\\"col-2 text-right p-2\\\"></div>\\n          </div>\\n          <!--Name-->\\n\\n\\n\\n          <!--Email Address-->\\n          <div class=\\\"row\\\" style=\\\"position:relative\\\">\\n            <div class=\\\"col-2 colorlightGray p-2\\\">email</div>\\n            <div class=\\\"col-8 text-right pl-2 pt-2 pb-2 pr-0\\\">\\n              <span #emailElement *ngIf=\\\"!input_Email\\\">{{myAccountModel.userData.emailId}}</span>\\n            </div>\\n          </div>\\n          <!--Email Address-->\\n\\n\\n\\n          <!--Password : Starts-->\\n          <div class=\